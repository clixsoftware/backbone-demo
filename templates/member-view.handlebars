<!-- READ VIEW -->
<p class="avatar">
	<img src="img/members/{{member.username}}.jpg" />
</p>
<dl class="dl-horizontal">
	<dt>Name:</dt>
	<dd>{{member.name}}</dd>
	<dt>Username:</dt>
	<dd>{{member.username}}</dd>
	<dt>Id:</dt>
	<dd>{{member.id}}</dd>
	<dt>Type:</dt>
	<dd>{{member.type}}</dd>
	<dt>City:</dt>
	<dd>{{member.city}}</dd>
	<dt>State:</dt>
	<dd>{{member.state}}</dd>
	<dt>Country:</dt>
	<dd>{{member.country}}</dd>
</dl>